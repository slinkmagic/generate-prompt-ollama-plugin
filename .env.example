# Ollama API設定
OLLAMA_ENDPOINT=http://localhost:11434
OLLAMA_MODEL=openhermes
OLLAMA_TIMEOUT=30

# プラグイン設定
PLUGIN_ENABLED=true
PROMPT_ENHANCEMENT_ENABLED=true
BATCH_PROCESSING_ENABLED=true
MAX_CONCURRENT_REQUESTS=3

# ログ設定
LOG_LEVEL=INFO
LOG_FILE=logs/ollama_plugin.log

# セキュリティ設定
SECURE_MODE=true
API_KEY_ENCRYPTION=true
EOF < /dev/null
